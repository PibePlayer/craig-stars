<Project Sdk="Godot.NET.Sdk/3.2.3">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <RootNamespace>CraigStars</RootNamespace>
    <GodotUseNETFrameworkRefAssemblies>true</GodotUseNETFrameworkRefAssemblies>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies.net472" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <None Include="**/*.tscn" />
    <None Remove="Core\**" />
    <None Remove="Core.Tests\**" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="src\Core\**" />
    <Compile Remove="src\Core.Tests\**" />
    <Compile Remove="Core\**" />
    <Compile Remove="Core.Tests\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="src\Core\**" />
    <EmbeddedResource Remove="src\Core.Tests\**" />
    <EmbeddedResource Remove="Core\**" />
    <EmbeddedResource Remove="Core.Tests\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="src\Core\Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.12">
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3">
    </PackageReference>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="15.5.0" />
    <PackageReference Include="NUnit" Version="3.9.0" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.9.0" />
  </ItemGroup>
</Project>